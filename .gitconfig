[core]
    pager = delta
[delta]
    plus-style = "syntax #012800"
    minus-style = "syntax #340001"
    syntax-theme = Monokai Extended
    navigate = true
[interactive]
    diffFilter = delta --color-only
[merge]
    conflictStyle = diff3
[sendemail]
	smtpserver = /usr/bin/msmtp
[commit]
	template = /home/silipwn/.gitmessages
